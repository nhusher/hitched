{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "key-spacing": ["error", { "align": "value" }],
    "no-else-return": 0,
    "no-multi-spaces": [ "error", { "exceptions": { "VariableDeclarator": true }}],
    "no-underscore-dangle": 0,
    "one-var": 0
  },
  "globals": {
    "Typekit": false
  }
}
